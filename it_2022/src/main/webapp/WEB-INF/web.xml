<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>LoggedAccessFilter</filter-name>
  	<display-name>LoggedAccessFilter</display-name>
  	<description></description>
  	<filter-class>filters.LoggedAccessFilter</filter-class>
  </filter>
  <filter>
  	<filter-name>ProfileAccessFilter</filter-name>
  	<display-name>ProfileAccessFilter</display-name>
  	<description></description>
  	<filter-class>filters.ProfileAccessFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>LoggedAccessFilter</filter-name>
  	<url-pattern>/LoggedAccessFilter</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>ProfileAccessFilter</filter-name>
  	<url-pattern>/ProfileAccessFilter</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<display-name>RegistrationServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserServlet</servlet-name>
  	<display-name>UserServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.UserServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CookieServlet</servlet-name>
  	<display-name>CookieServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.CookieServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ErrorHandlerServlet</servlet-name>
  	<display-name>ErrorHandlerServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.ErrorHandlerServlet</servlet-class>
  </servlet>
<error-page>
<error-code>404</error-code>
<location>/error</location>
</error-page>
<error-page>
<error-code>403</error-code>
<location>/error</location>
</error-page>
<error-page>
<error-code>500</error-code>
<location>/error</location>
</error-page>
</web-app>
